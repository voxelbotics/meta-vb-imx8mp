                        self-powered;

                        ports {
                                #address-cells = <1>;
                                #size-cells = <0>;

                                port@1 {
                                        reg = <1>;
                                        typec_con_ss_port2: endpoint {
                                                remote-endpoint = <&usb3_data_ss_port2>;
                                        };
                                };
                        };
                };
        };



